{
  "version": 2,
  "name": "voicednut-miniapp",
  "builds": [
    {
      "src": "server/webapp.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/miniapp.html",
      "dest": "/public/miniapp.html"
    },
    {
      "src": "/",
      "dest": "/server/webapp.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/server/webapp.js"
    },
    {
      "src": "/debug",
      "dest": "/server/webapp.js"
    },
    {
      "src": "/public/(.*)",
      "dest": "/public/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "server/webapp.js": {
      "maxDuration": 30
    }
  }
}
