{
  "name": "voicednut-miniapp",
  "version": "1.0.0",
  "description": "Telegram Mini App for Voicednut Bot",
  "private": true,
  "type": "module",
  "main": "server/webapp.js",
  "scripts": {
    "start": "node server/webapp.js",
    "dev": "vite",
    "dev:https": "cross-env HTTPS=true vite",
    "build": "tsc --noEmit && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
    "preview": "vite preview",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel",
    "logs": "vercel logs"
  },
  "dependencies": {
    "@telegram-apps/sdk-react": "^3.3.6",
    "@telegram-apps/telegram-ui": "^2.1.9",
    "@tonconnect/ui-react": "^2.2.0",
    "axios": "^1.6.2",
    "chart.js": "^4.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "eruda": "^3.4.3",
    "express": "^4.18.2",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/node": "^20.19.11",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.34.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^15.15.0",
    "nodemon": "^3.0.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0",
    "vercel": "^32.0.0",
    "vite": "^6.3.5",
    "vite-plugin-mkcert": "^1.17.8",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "telegram-miniapp",
    "voice-calls",
    "sms",
    "ai"
  ],
  "author": "Tunde Ednut",
  "license": "MIT"
}
