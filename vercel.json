{
  "version": 2,
  "builds": [
    {
      "src": "bot/miniapp/server/webapp.js",
      "use": "@vercel/node"
    },
    {
      "src": "bot/miniapp/public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/miniapp.html",
      "dest": "bot/miniapp/public/miniapp.html"
    },
    {
      "src": "/",
      "dest": "bot/miniapp/server/webapp.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "bot/miniapp/server/webapp.js"
    },
    {
      "src": "/debug",
      "dest": "bot/miniapp/server/webapp.js"
    },
    {
      "src": "/public/(.*)",
      "dest": "bot/miniapp/public/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
