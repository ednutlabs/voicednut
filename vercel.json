{
  "version": 2,
  "name": "voicednut-miniapp",
  "routes": [
    {
      "src": "/miniapp.html",
      "dest": "/bot/public/miniapp.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "/bot/server/webapp.js"
    },
    {
      "src": "/(.*)",
      "dest": "/bot/server/webapp.js"
    }
  ],
  "functions": {
    "bot/server/webapp.js": {
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}