{
  "version": 2,
  "builds": [
    {
      "src": "bot/server/webapp.js",
      "use": "@vercel/node"
    },
    {
      "src": "bot/public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/miniapp.html",
      "dest": "/bot/public/miniapp.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "/bot/server/webapp.js"
    },
    {
      "src": "/(.*)",
      "dest": "/bot/server/webapp.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
