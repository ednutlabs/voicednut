{
  "version": 2,
  "name": "voicednut-miniapp",
  "builds": [
    {
      "src": "mini-app/server/webapp.js",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb"
      }
    },
    {
      "src": "mini-app/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "mini-app/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "mini-app/server/webapp.js"
    },
    {
      "src": "/debug",
      "dest": "mini-app/server/webapp.js"
    },
    {
      "src": "/miniapp.html",
      "dest": "mini-app/server/webapp.js"
    },
    {
      "src": "/ws",
      "dest": "mini-app/server/webapp.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "mini-app/dist/assets/$1"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "dest": "mini-app/dist/$1"
    },
    {
      "src": "/",
      "dest": "mini-app/dist/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "mini-app/dist/index.html"
    }
  ],
  "functions": {
    "mini-app/server/webapp.js": {
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "installCommand": "cd bot/mini-app && npm install",
  "buildCommand": "cd bot/mini-app && npm run build",
  "outputDirectory": "bot/mini-app/dist",
  "framework": "vite"
}